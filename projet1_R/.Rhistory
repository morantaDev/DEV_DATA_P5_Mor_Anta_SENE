valid_note <- function(argument){
splt <- str_split(argument, "#")[[1]]
resultat <- vector("list", length(splt))
for(i in seq_along(splt)){
matiere_note <- str_split(splt[i], "\\[|\\]")[[1]]
matiere <- matiere_note[1]
note <- matiere_note[2]
if(!is.numeric(note)){
note <- str_replace(note, ",", ".") # remplacer une éventuelle virgule par un point pour que R considère la note comme un nombre réel
}
is_valid_note <- is.numeric(note)
resultat[[i]] <- list(matiere = matiere, note = note, is_valid_note = is_valid_note)
}
return(resultat)
}
